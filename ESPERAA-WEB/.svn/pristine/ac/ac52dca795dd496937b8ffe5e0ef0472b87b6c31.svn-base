@CHARSET "ISO-8859-1";

.profilInfos, .profilInfos > fieldset {
	max-height : 100%;
	overflow: auto;
	border: 1px solid #ccc;
  	padding: 10px;	
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px; 
}

.profilInfos > legend{
	width:inherit; 
    padding:0 10px; 
    border-bottom:none; 
}

fieldset > fieldset{
	border: 1px solid #ccc;
  	padding: 10px;	
	margin-top: 10px;
}